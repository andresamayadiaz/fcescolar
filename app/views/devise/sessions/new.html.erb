    <div class='middle-container'>
      <div class='middle-row'>
        <div class='middle-wrapper'>
          <div class='login-container-header'>
            <div class='container'>
              <div class='row'>
                <div class='col-sm-12'>
                  <%= render 'layouts/messages' %>
                  <div class='text-center'>
					<%= image_tag("flatty/flogo_lg.png", height: '93', width: '80') %>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class='login-container'>
            <div class='container'>
              <div class='row'>
                <div class='col-sm-4 col-sm-offset-4'>
                  <h1 class='text-center title'>Ingresar</h1>
				  <%= form_for(User.new, :as => :user, :url => session_path(:user), :html => { :role => 'form', :class => 'validate-form'}) do |f| %>
                    <div class='form-group'>
                      <div class='controls with-icon-over-input'>
						<%= f.email_field :email, :autofocus => true, class: 'form-control', placeholder: 'E-mail', 'data-rule-required' => 'true' %>
                        <i class='icon-user text-muted'></i>
                      </div>
                    </div>
                    <div class='form-group'>
                      <div class='controls with-icon-over-input'>
						<%= f.password_field :password, class: 'form-control', placeholder: 'Password', 'data-rule-required' => 'true' %>
                        <i class='icon-lock text-muted'></i>
                      </div>
                    </div>
                    <div class='checkbox'>
                      <label for='remember_me'>
                        <input id='remember_me' name='remember_me' type='checkbox' value='1'>
                        Recordar Usuario
                      </label>
                    </div>
                    <button class='btn btn-block'>Entrar</button>
                  <% end %>
                  <div class='text-center'>
                    <hr class='hr-normal'>
                    <a href='<%= new_user_password_path %>'>Â¿Olvidaste tu Password?</a>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class='login-container-footer'>
            <div class='container'>
              <div class='row'>
                <div class='col-sm-12'>
                  <div class='text-center'>
                    <a href='sign_up.html'>
                      <i class='icon-user'></i>
                      Reactivar Cuenta
                    </a>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
