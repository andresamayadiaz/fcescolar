<div class="form-group">
    <label for="curricular_line" class="col-xs-2 control-label">Curricular Line</label>
    <div class="col-xs-10">
        <div class="form-inline">
	        <%= f.input :curricular_line_id, collection: CurricularLine.all, label:false %>
        </div>
    </div>
</div>
<div class="form-group">
    <label for="subject" class="col-xs-2 control-label">Subject</label>
    <div class="col-xs-10">
        <div class="form-inline">
	        <%= f.input :subject_id, collection: Subject.all, label:false %>
        </div>
    </div>
</div>
<div class="clear"></div>
<%= f.input :weekly_frequency %>
<%= f.input :credits %>
<div class="form-group">
  <%= link_to_remove_association "x", f %>
</div>
