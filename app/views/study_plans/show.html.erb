<% if @is_pdf %>
  <meta http-equiv="content-type" content="text/html; charset=utf-8" />
<% end %>
<h1><%= @study_plan.name %></h1>
<% if @enabled %>
  <p><%= t('general.enabled') %></p>
  <%= render 'show_enabled' %>
<% else %>
  <p><%= t('general.not_enabled') %></p>
  <%= render 'show_not_enabled' %>
<% end %>
