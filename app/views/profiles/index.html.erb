<h1>Listing profiles</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Last academic degree</th>
      <th>Country</th>
      <th>State</th>
      <th>Street</th>
      <th>External</th>
      <th>Internal</th>
      <th>Colonia</th>
      <th>Cp</th>
      <th>City</th>
      <th>Emergency phone</th>
      <th>Additional email 1</th>
      <th>Additional email 2</th>
      <th>Phones 1</th>
      <th>Phone type 1</th>
      <th>Phones 2</th>
      <th>Phone type 2</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profiles.each do |profile| %>
      <tr>
        <td><%= profile.user %></td>
        <td><%= profile.last_academic_degree %></td>
        <td><%= profile.country %></td>
        <td><%= profile.state %></td>
        <td><%= profile.street %></td>
        <td><%= profile.external %></td>
        <td><%= profile.internal %></td>
        <td><%= profile.colonia %></td>
        <td><%= profile.cp %></td>
        <td><%= profile.city %></td>
        <td><%= profile.emergency_phone %></td>
        <td><%= profile.additional_email_1 %></td>
        <td><%= profile.additional_email_2 %></td>
        <td><%= profile.phones_1 %></td>
        <td><%= profile.phone_type_1 %></td>
        <td><%= profile.phones_2 %></td>
        <td><%= profile.phone_type_2 %></td>
        <td><%= link_to 'Show', profile %></td>
        <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
        <td><%= link_to 'Destroy', profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profile', new_profile_path %>
