  <!-- // TITULO -->
  <div class="row">
    <div class="col-sm-12">
      <div class="page-header">
        <h1 class="pull-left">
          <i class="icon-list"></i>
          <span>Catálogo de Formas de Titulación</span>
        </h1>
        <div class="pull-right">
          <ul class="breadcrumb">
            <li>
              <a href="index.html">
                <i class="icon-home"></i>
              </a>
            </li>
            <li class="separator">
              <i class="icon-angle-right"></i>
            </li>
            <li>
              Catálogos
            </li>
            <li class="separator">
              <i class="icon-angle-right"></i>
            </li>
            <li class="active">Formas de Titulación</li>
          </ul>
        </div>
      </div>
    </div>
  </div>
  <!-- .// TITULO -->

  <!-- // TABLE -->
  <div class='box bordered-box blue-border' style='margin-bottom:0;'>
    <div class='box-header blue-background'>
      <div class='title'>Listado de Formas de Titulación</div>
      <div class='actions'>
        <%= link_to 'Nuevo', new_degree_achievements_category_path, :class=>'btn btn-large btn-success' %>
      </div>
    </div>
    <div class='box-content box-no-padding'>
      <div class='responsive-table'>
        <div class='scrollable-area'>
          <table class='data-table table table-bordered table-striped' style='margin-bottom:0;'>
            <thead>
              <tr>
                <th>
                  Franquicia
                </th>
                <th>
                  Nombre
                </th>
                <th></th>
              </tr>
            </thead>
            <tbody>
              <% @degree_achievements_categories.each do |degree_achievements_category| %>
              <tr>
                <td><%= degree_achievements_category.franchise.try(:name) %></td>
                <td><%= degree_achievements_category.name %></td>
                <td>
                  <div class="text-right">
                  <%= link_to edit_degree_achievements_category_url(degree_achievements_category), class: 'btn btn-success btn-xs' do %>
                    <i class='icon-ok'></i>
                    <% end %>
                    <%= link_to degree_achievements_category, class: 'btn btn-danger btn-xs', method: :delete, data: { confirm: 'Are you sure?' } do %>
                    <i class='icon-remove'></i>
                    <% end %>
                  </div>
                </td>
              </tr>
              <% end %>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
  <!-- .// TABLE -->
  
